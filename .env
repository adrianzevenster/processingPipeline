
# Path to your GCP service account JSON (optional)
# If omitted, will use Application Default Credentials from gcloud SDK
GOOGLE_APPLICATION_CREDENTIALS=./service-account.json
# GCP Document AI settings (optional if ADC project is set)
GCP_PROJECT_ID=your-gcp-project-id
GCP_LOCATION=us
GCP_PROCESSOR_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
# Gemini API settings
GEMINI_ENDPOINT_URL=https://api.gemini.example/v1/analyze
GEMINI_API_KEY=YOUR_API_KEY_HERE
# Input and output directories
INPUT_DIR=./data/input
OUTPUT_DIR=./data/output
